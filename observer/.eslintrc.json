{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:node/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [1, "ignorePackages"],
        "no-restricted-syntax": [1, "ForInStatement", "LabeledStatement", "WithStatement"],
        "semi": [1, "never"],
        "comma-dangle": [1, "never"],
        "linebreak-style": "off",
        "indent": ["error", 4],
        "object-curly-newline": ["error", { "consistent": true }],
        "no-underscore-dangle":  "off"
    }
}
